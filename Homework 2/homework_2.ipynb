    "# 3. Handle MasVnrType and MasVnrArea\n",
    "train_df['MasVnrType'] = train_df['MasVnrType'].fillna('None')\n",
    "train_df.loc[train_df['MasVnrType'] == 'None', 'MasVnrArea'] = 0\n",
